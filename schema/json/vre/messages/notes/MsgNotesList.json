{
  "$schema": "http://json-schema.org/schema",
  "$id": "https://dharpa.org/schemas/vre/messages/notes/MsgNotesList.json",
  "type": "object",
  "description": "Contains list of notes for a workflow step.",
  "additionalProperties": false,
  "properties": {
    "stepId": {
      "type": "string",
      "description": "Workflow step Id."
    },
    "notes": {
      "type": "array",
      "items": {
        "$ref": "https://dharpa.org/schemas/backend/Note.json"
      }
    }
  },
  "required": [
    "stepId",
    "notes"
  ]
}