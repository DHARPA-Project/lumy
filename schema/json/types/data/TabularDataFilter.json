{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "additionalProperties": false,
  "title": "DataTabularDataFilter",
  "description": "Filter for tabular data",
  "properties": {
    "pageSize": {
      "type": "integer",
      "description": "Size of the page"
    },
    "offset": {
      "type": "integer",
      "description": "Offset of the page"
    },
    "fullValue": {
      "type": "boolean",
      "description": "Whether to ignore other filter items and return full value."
    },
    "condition": {
      "$ref": "https://dharpa.org/schema/types/data/TabularDataFilterCondition.json"
    },
    "sorting": {
      "$ref": "https://dharpa.org/schema/types/data/TabularDataSortingMethod.json"
    }
  },
  "required": []
}