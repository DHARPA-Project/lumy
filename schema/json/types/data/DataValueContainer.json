{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "additionalProperties": false,
  "title": "DataValueContainer",
  "description": "Used to send serialized data from front end to back end.",
  "properties": {
    "dataType": {
      "type": "string",
      "enum": [
        "table",
        "simple"
      ],
      "description": "Type of the data value."
    },
    "value": {
      "description": "Actual serialized value."
    }
  },
  "required": [
    "dataType"
  ]
}